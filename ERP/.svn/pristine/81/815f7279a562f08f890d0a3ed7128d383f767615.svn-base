<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>    
<%@ taglib uri="http://www.springframework.org/tags" prefix="spring" %>  
<h1 style="padding-left: 30px; padding-top: 25px;">부서별 인사현황 </h1>  

<div>
<table class="table table-bordered  text-left">
	<thead class="">
		<tr>
			<td>부서 </td>
			<td>1팀 </td>
			<td>2팀 </td>
			<td>3팀 </td>
			<td>총인원 </td>
		</tr>
	</thead>
	<tbody>
		<c:if test="${not empty acList }">
			<tr>
				<td>회계부서</td>
			<c:forEach items="${acList }" var="ac">
				<td>${ac }</td>
			</c:forEach>
			</tr>
		</c:if>
		<c:if test="${not empty buyList }">
			<tr>
				<td>구매부서</td>
			<c:forEach items="${buyList }" var="ac">
				<td>${ac }</td>
			</c:forEach>
			</tr>
		</c:if>
		<c:if test="${not empty whList }">
			<tr>
				<td>자재부서</td>
			<c:forEach items="${whList }" var="ac">
				<td>${ac }</td>
			</c:forEach>
			</tr>
		</c:if>
		<c:if test="${not empty productList }">
			<tr>
				<td>생산부서</td>
			<c:forEach items="${productList }" var="ac">
				<td>${ac }</td>
			</c:forEach>
			</tr>
		</c:if>
		<c:if test="${not empty orderList }">
			<tr>
				<td>영업부서</td>
			<c:forEach items="${orderList }" var="ac">
				<td>${ac }</td>
			</c:forEach>
			</tr>
		</c:if>
		<c:if test="${not empty empList }">
			<tr>
				<td>인사부서</td>
			<c:forEach items="${empList }" var="ac">
				<td>${ac }</td>
			</c:forEach>
			</tr>
		</c:if>
	</tbody>
</table>
</div>
<tr class="row">
	<td class="col text-right">
	<input class="  mr-2" type="text" value="집계 기준일 : ${System.currentTimeMillis()}"> 
	</td>
</tr>

<input type="button" value="클릭" id="test"/>




